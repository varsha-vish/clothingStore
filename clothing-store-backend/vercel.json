{
  "version": 2,
  "builds": [
    {
      "src": "main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "nest build",
  "outputDirectory": "dist"
}